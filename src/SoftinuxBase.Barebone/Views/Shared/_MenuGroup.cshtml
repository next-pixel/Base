@model SoftinuxBase.Barebone.ViewModels.Shared.MenuGroup.MenuGroupViewModel

<li>
    <a href="#" class="dropdown-toggle">
          <span class="icon"><i class="@(Model.FontAwesomeClass)"></i></span>
          <span class="caption">@(Model.Name)</span>
      </a>
    <ul class="navview-menu stay-open" data-role="dropdown">
        <li class="item-header">@(Model.Name)</li>
        @foreach (var menuItem in Model.MenuItems)
        {
            <partial name="_MenuItem" model="menuItem"/>
        }
    </ul>
</li>





